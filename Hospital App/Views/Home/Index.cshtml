@model Hospital_App.Models.Business


<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta name="description" content="">
    <meta name="author" content="">

    <title>Medic Care Bootstrap 5 CSS Template</title>

    <!-- CSS FILES -->
    <link rel="preconnect" href="https://fonts.googleapis.com">

    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600;700&display=swap" rel="stylesheet">

    <link href="~/medic-care/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/medic-care/css/bootstrap-icons.css" rel="stylesheet">

    <link href="~/medic-care/css/owl.carousel.min.css" rel="stylesheet">

    <link href="~/medic-care/css/owl.theme.default.min.css" rel="stylesheet">

    <link href="~/medic-care/css/templatemo-medic-care.css" rel="stylesheet">
    <!--

    TemplateMo 566 Medic Care

    https://templatemo.com/tm-566-medic-care

    -->
</head>

<body id="top">

    <main>

        <nav class="navbar navbar-expand-lg bg-light fixed-top shadow-lg">
            <div class="container">
                <a class="navbar-brand mx-auto d-lg-none" href="index.html">
                    Medic Care
                    <strong class="d-block">Health Specialist</strong>
                </a>

                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav mx-auto">
                        <a class="navbar-brand d-none d-lg-block" href="index.html">
                            Helping Hands
                            <strong class="d-block">Wound Care</strong>
                        </a>
                        <li class="nav-item active">
                            <a class="nav-link" href="#hero">Home</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="#about">About</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="#contact">Contact</a>
                        </li>



                     

                        <li class="nav-item">
                            <a class="nav-link" href="#map">Location</a>
                        </li>

                        <li class="nav-item">
                            <partial name="_LoginPartial" />
                        </li>
                    </ul>
                </div>

            </div>
        </nav>

        <section class="hero" id="hero">
            <div class="container">
                <div class="row">

                    <div class="col-12">
                        <div id="myCarousel" class="carousel slide carousel-fade" data-bs-ride="carousel">
                            <div class="carousel-inner">
                                <div class="carousel-item active">
                                    <img src="~/medic-care/images/slider/portrait-successful-mid-adult-doctor-with-crossed-arms.jpg" class="img-fluid" alt="">
                                </div>

                                <div class="carousel-item">
                                    <img src="~/medic-care/images/slider/young-asian-female-dentist-white-coat-posing-clinic-equipment.jpg" class="img-fluid" alt="">
                                </div>

                                <div class="carousel-item">
                                    <img src="~/medic-care/images/slider/doctor-s-hand-holding-stethoscope-closeup.jpg" class="img-fluid" alt="">
                                </div>
                            </div>
                        </div>

                        <div class="heroText d-flex flex-column justify-content-center">

                            <h1 class="mt-auto mb-2">
                                Better
                                <div class="animated-info">
                                    <span class="animated-item">health</span>
                                    <span class="animated-item">days</span>
                                    <span class="animated-item">lives</span>
                                </div>
                            </h1>

                            <p class="mb-4">Threat your wound with us</p>

                           
                        </div>
                    </div>

                </div>
            </div>
        </section>

        <section class="section-padding" id="about">
            <div class="container">
                <div class="row">

                    <div class="col-lg-6 col-md-6 col-12">
                        <h2 class="mb-lg-3 mb-3">
                            About Us

                        </h2>

                        <p>Welcome to Helping Hands Wound Care, where compassion meets healing. Our non-profit organization is dedicated to providing essential care and support to patients with wounds, fostering a community committed to the well-being of those in need.</p>

                        <p> We specialize in the treatment of various wounds, including surgical wounds, diabetic ulcers, pressure sores, and more. Our experienced team of healthcare professionals works tirelessly to provide personalized care and innovative solutions for optimal healing.</p>
                    </div>

                    <div class="col-lg-4 col-md-5 col-12 mx-auto">
                        <div class="featured-circle bg-white shadow-lg d-flex justify-content-center align-items-center">
                            <p class="featured-text"><span class="featured-number">12</span> Years<br> of Experiences</p>
                        </div>
                    </div>

                </div>
            </div>
        </section>

        <section class="gallery">
            <div class="container">
                <div class="row">


                </div>
            </div>
        </section>

        

       
        <section class="section-padding" id="map">
            <div class="container">
                <div class="row">
                    <h2 class="text-center mb-lg-3 mb-2">Location</h2>

                    <div class="col-12">
                        <iframe width="100%"
                                height="450"
                                style="border:0"
                                loading="lazy"
                                allowfullscreen
                                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3309.2749745989795!2d25.61826847491229!3d-33.95977122377512!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x1e7ad2f47a5f95b1%3A0x52188405bffa8b46!2s88%20Govan%20Mbeki%20St%2C%20Port%20Elizabeth%20Central%2C%20Gqeberha%2C%206001!5e0!3m2!1sen!2sza!4v1698425574958!5m2!1sen!2sza">
                        </iframe>
                    </div>
                </div>
            </div>
        </section>



    </main>

    <footer class="site-footer section-padding" id="contact">
        <div class="container">
            <div class="row">

                <div class="col-lg-5 me-auto col-10">
                    <h5 class="mb-lg-4 mb-3">Opening Hours</h5>

                    <ul class="list-group list-group-flush">
                        <li class="list-group-item d-flex">
                            Sunday : Closed
                        </li>

                        <li class="list-group-item d-flex">
                            Monday - Firday
                            <span>@Model.OperatingHours</span>
                        </li>

                        <li class="list-group-item d-flex">
                            Saturday : Closed
                        </li>
                    </ul>
                </div>
                <div class="col-lg-2 col-md-6 col-12 my-4 my-lg-0">
                    <h5 class="mb-lg-4 mb-3">Our Contact</h5>

                    <p><a href="mailto:hello@company.co">@Model.Email</a><p>
                    <p>@Model.ConatactNumber</p>

                </div>
                <div class="col-lg-2 col-md-6 col-12 my-4 my-lg-0">
                    <h5 class="mb-lg-4 mb-3">Address</h5>

                    <p>@Model.AddressLine1</p>
                    <p>@Model.AddressLine2</p>
                    <p>@Model.Suburb.SuburbName</p>
                    <p>@Model.Suburb.PostalCode</p>
                </div>

                <div class="col-lg-3 col-md-6 col-12 ms-auto">
                    <h5 class="mb-lg-4 mb-2">Socials</h5>

                    <ul class="social-icon">
                        <li><a href="#" class="social-icon-link bi-facebook"></a></li>

                        <li><a href="#" class="social-icon-link bi-twitter"></a></li>

                        <li><a href="#" class="social-icon-link bi-instagram"></a></li>

                        <li><a href="#" class="social-icon-link bi-youtube"></a></li>
                    </ul>

                    <div>
                        <p class="copyright-text">
                            Copyright © Helping Hands 2023
                            <br><br>Design: <a href="https://templatemo.com" target="_parent">TemplateMo</a>
                        </p>
                        <p class="copyright-text">Distributed By: <a href="https://themewagon.com" target="_parent">Themewagon</a></p>
                    </div>
                </div>



            </div>
            </section>
    </footer>

    <!-- JAVASCRIPT FILES -->
    <script src="~/medic-care/js/jquery.min.js"></script>
    <script src="~/medic-care/js/bootstrap.bundle.min.js"></script>
    <script src="~/medic-care/js/owl.carousel.min.js"></script>
    <script src="~/medic-care/js/scrollspy.min.js"></script>
    <script src="~/medic-care/js/custom.js"></script>
    <!--

    TemplateMo 566 Medic Care

    https://templatemo.com/tm-566-medic-care

    -->
</body>
</html>