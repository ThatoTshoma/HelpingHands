@model IEnumerable<Chronic>
@{
    ViewData["Title"] = "List Of Chronic Condition";
    Layout = "~/Views/Shared/_OfficeManagerLayout.cshtml";
}
<div class="row">
    <ol class="breadcrumb">
        <li>
            <a href="#">
                <em class="fa fa-home"></em>
            </a>
        </li>
        <li class="active">Chronic Condition</li>
        <li class="active">View Chronic Condition</li>
    </ol>
</div>
<div class="container px-6 mx-auto grid">

<h2><b>@ViewData["Title"]</b></h2>

<p>
    <a asp-action="AddChromin" asp-controller="Admin" class="btn btn-primary">Create New</a>
</p>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Chronic Name</th>
            <th>Description</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var chronic in Model)
        {
            <tr>
                <td>@chronic.ChronicName</td>
                <td>@chronic.Description</td>


            </tr>
        }
    </tbody>
</table>
</div>